<span class='input-group-addon'><i class="fa fa-book"></i></span>
<select id='note-notebook' name='note[notebook_id]' class='selectpicker'>
  <% notebooks.each( function(notebook) { %>
    <option
    value="<%= notebook.id %>"
    <% if (notebook.id === nbid) { %>
      selected
    <% } %>
    ><%= notebook.get('title') %></option>
  <% }) %>
</select>
